<script>
function gaAddToCart(sku, name) {
    ga('require', 'ec');
    ga('ec:addProduct', {
        'id': sku,
        'name': name
      });
      ga('ec:setAction', 'add');
      ga('send', 'event', 'UX', 'click', 'add to cart');     // Send data using an event.
}
</script>    </div>

